&general n_ci=5,n_ci_read=5 
mol_cc= 0. 0. 0.
medium='sol'
n_f=1 
dt=0.20
n_step=5000
n_out=1
/ 

&field 
Ffld='snd'
omega(1)=0.5 
sigma(1)=10.,
t_mid=500 
radiative='non'
npulse=1
fmax(1,1)= 0. 
fmax(1,1)=0. 
fmax(1,3)=0.000001      
/

&sse dissipative='non'
/

&spectra start=80, 
tau= 2000.0 2000.0
dir_ft= 1. 1. 1. 
/

